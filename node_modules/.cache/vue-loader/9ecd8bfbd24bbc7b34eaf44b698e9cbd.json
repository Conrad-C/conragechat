this.socket.on('message',(data)=>{\n        this.messages.push(data);\n        if(data.id != this.id){\n          this.storeHighScore(data)\n        }\n        this.scrollToElement();\n    })\n  },\n}\n</script>\n\n<style>\n*{\n  border: none;\n  padding: 0px;\n  margin: 0px;\n  overflow: hidden;\n}\n:root{\n  --manterblack:black;\n  --main-color:rgb(255, 248, 244);\n  --invert:invert(none);\n  --background:rgb(255, 254, 254);\n  --invert-white:rgb(255, 254, 254);\n  --manterwhite:rgb(255, 254, 254);\n  --orange:rgb(255, 112, 29);\n  --blue:#0384CF;\n}\n#app{\n  width: 100%;\n  height: 100vh;\n  display: flex;\n  align-items: center;\n  justify-content: center;\n}\n.left-side{\n  width: 100%;\n  display: flex;\n  flex-direction: column;\n  align-items: flex-start;\n}\n.right-side{\n  width: 100%;\n  display: flex;\n  flex-direction: column;\n  align-items: flex-end;\n}\n.send-message{\n  cursor: pointer;\n  margin-left: 20px;\n  background: white;\n  border-radius: 40px;\n  width: 40px;\n  height: 40px;\n  outline: none;\n  border:none;\n  color: rgb(151, 128, 172);\n  transition: 0.3s;\n  font-size: 30px;\n}\n.send-message:hover{\n  background: rgb(247, 245, 245);\n}\n.input-body{\n  align-items: center;\n  justify-content: center;\n  width: 100%;\n  min-height: 70px;\n  display: flex;\n  flex-direction: row;\n  height: 70px;\n  background: rgb(151, 128, 172);\n}\n.input{\n  height: 40px;\n  width: 70%;\n  border-radius: 30px;\n  outline: none;\n  color: rgb(68, 65, 65);\n  text-indent: 30px;\n  border: 1px solid rgb(156, 154, 154);\n  background: rgb(182, 157, 206);\n  transition-duration: 0.3s;\n  -webkit-transition-duration: 0.3s;\n}\n  .input::placeholder{\n    color: rgb(92, 91, 89);\n  }\n  .input:focus{\n    border: 1px solid white;\n    background: rgb(198, 174, 211);\n  }\n</style>\n"]}]}